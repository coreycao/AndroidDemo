<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.corey.customview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".edittext.EditTextActivity" />

        <activity android:name=".gcssloop.CanvasActivity" />

        <activity android:name=".layout_tips.LayoutActivity" />

        <activity android:name=".devart.DevArtActivity" />

        <activity android:name=".anim.AnimActivity" />

        <activity android:name=".ZoomActivity" />
        <activity android:name=".chart.ChartActivity" />
        <activity android:name=".DetailActivity" />
        <activity android:name=".pager.PagerActivity" />
    </application>

</manifest>
